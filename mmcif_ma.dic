## MA_PROTOCOL_STEP         ##
##############################

save_ma_protocol_step
    _category.description
;              Data items in the MA_PROTOCOL_STEP category captures the
               details of the modeling protocol and individial steps 
               within each protocol. 
;
    _category.id                  ma_protocol_step
    _category.mandatory_code      yes
     loop_
    _category_key.name          '_ma_protocol_step.ordinal_id'

    loop_
    _category_group.id           'inclusive_group'
                                 'ma_group'
    loop_
    _category_examples.detail
    _category_examples.case
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
;
    Example 1 -
;
;
  loop_
  _ma_protocol_step.ordinal_id
  _ma_protocol_step.protocol_id
  _ma_protocol_step.step_id
  _ma_protocol_step.method_type
  _ma_protocol_step.step_name
  _ma_protocol_step.software_id
  _ma_protocol_step.input_data_group_id
  _ma_protocol_step.output_data_group_id

   0 0 0 "template search" . 2   1  5
   1 0 1 "template search" . 2   2  6
   2 0 2 "template search" . 2   3  7
   3 0 3 "template search" . 2   4  8
   4 1 0 "template selection" . 2  5  9
   5 1 1 "template selection" . 2  6 10
   6 1 2 "template selection" . 2  7 11
   7 1 3 "template selection" . 2  8 12
   8 2 0 "modeling" . 2   13   14
   9 3 0 "model selection" . 2   14  15
  # ... abbreviated ... #
;
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
   _pdbx_category_context.type         CONTRIBUTED
   _pdbx_category_context.category_id  ma_protocol_step
     save_
   
