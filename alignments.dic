##############################
## MA_ALIGNMENT             ##
##############################

save_ma_alignment
    _category.description
;              Data items in the MA_ALIGNMENT category record details about
               the relationship of homologous sequences to the target sequence
			   alignments can be fully gapped or partial with the corresponding 
			   from, -to annotations. Further details are stored in data items
			   MA_ALIGNMENT_SEGMENT, MA_TEMPLATE and MA_TEMPLATE_ATOM_SITE
			   
;
    _category.id                  ma_alignment
    _category.mandatory_code      no
    _category_key.name          'ma_alignment.id'
    loop_
    _category_group.id           ?
    loop_
    _category_examples.detail
    _category_examples.case
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
;
;Example
  loop_
	ma_alignment.ordinal_id
	ma_alignment.id
	ma_alignment.template_ordinal_id
	ma_alignment.from
	ma_alignment.to
	ma_alignment.target_sequence

	save_

0 0 0 1 100 >Target\nGAMGFGDLKSPAGLQVLNDYLA----DKSYIEGYVPSQADVAVFEAVSSP------PPADLCHALRWYNHIKSYEKEKASLPGVKKALGKYGPADVEDTT
0 0 1 30 90 >Target\nGAMGFGDLKSPAGLQVLNDYLA----DKSYIEGYVPSQADVAVFEAVSSP------PPADLCHALRWYNHIKSYEKEKASLPGVKKALGKYGPADVEDTT

;

   _pdbx_category_context.type         CONTRIBUTED
   _pdbx_category_context.category_id  ma_alignment
     save_


