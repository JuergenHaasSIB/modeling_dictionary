save_ma_step_input
    _category.description
;              Data items in the MA_step_input category captures the
               input data for a given protocol_step.   
;
    _category.id                  ma_step_input
    _category.mandatory_code      yes
     loop_
    _category_key.name          '_ma_step_input.id'

    loop_
    _category_group.id           'inclusive_group'
                                 'ma_group'
    loop_
    _category_examples.detail
    _category_examples.case
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
;
    Example 1 - 
;
;
        loop_
        _ma_step_input.id
        _ma_step_input.protocol_id
        _ma_step_input.step_id
        _ma_step_input.data_id
        _ma_step_input.content_sub_type
        0 0 0 0  "distance restraint"
        1 0 1 0  "dihedral restraint"
        2 0 2 1  "target-template pairwise alignment"
       # ...  abbreviated ...  
;
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
   _pdbx_category_context.type         CONTRIBUTED
   _pdbx_category_context.category_id  ma_step_input
     save_

save__ma_step_input.id
    _item_description.description
;    A unique identifier for the entry.
;
    _item.name                  '_ma_step_input.id'
    _item.category_id             ma_step_input
    _item.mandatory_code          yes
    _item_type.code               int
     save_ 

save__ma_step_input.protocol_id
    _item_description.description
;    An identifier to the protocol. 
     This data item is a pointer to _ma_protocol_step.protocol_id in the 
     MA_PROTOCOL_STEP category. 
;
    _item.name                  '_ma_step_input.protocol_id'
    _item.category_id             ma_step_input
    _item.mandatory_code          yes
    _item_type.code               int
    _item_linked.child_name     '_ma_step_input.protocol_id'
    _item_linked.parent_name    '_ma_protocol_step.protocol_id'
     save_

save__ma_step_input.step_id
    _item_description.description
;    An identifier to the protocol step. 
     This data item is a pointer to _ma_protocol_step.step_id in the 
     MA_PROTOCOL_STEP category. 
;
    _item.name                  '_ma_step_input.step_id'
    _item.category_id             ma_step_input
    _item.mandatory_code          yes
    _item_type.code               int
    _item_linked.child_name     '_ma_step_input.step_id'
