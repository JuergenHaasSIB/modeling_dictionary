    _item.category_id             ma_step_input
    _item.mandatory_code          yes
    _item_type.code               int
    _item_linked.child_name     '_ma_step_input.data_id'
    _item_linked.parent_name    '_ma_data.id'
     save

save__ma_step_input.content_sub_type
    _item_description.description
;    The sub-type of input content.
;
    _item.name                  '_ma_step_input.content_sub_type'
    _item.category_id             ma_step_input
