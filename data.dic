    Example 1 - 
;
;
        loop_
        _ma_data.id
        _ma_data.name
        _ma_data.content_type
        0 "NMR NOE Distances" "restraint" 
        1 "Target Template Alignment" "alignment" 
        2 "Coevolution Data" "alignment" 
         # ...  abbreviated ...
;
